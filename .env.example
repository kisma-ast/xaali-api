# Configuration des clés API - Copiez ce fichier vers .env et remplissez vos vraies clés

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Pinecone Configuration  
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=xaali-agent

# Bictorys Mobile Money Configuration
BICTORYS_MERCHANT_ID=your_merchant_id_here
BICTORYS_API_KEY=your_bictorys_api_key_here
BICTORYS_SECRET_KEY=your_bictorys_secret_key_here

# Production Bictorys (optionnel)
BICTORYS_PROD_MERCHANT_ID=your_prod_merchant_id_here
BICTORYS_PROD_API_KEY=your_prod_api_key_here
BICTORYS_PROD_SECRET_KEY=your_prod_secret_key_here

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=your_db_password_here
DB_DATABASE=xaali

# Application URLs
BACKEND_URL=http://localhost:3000
FRONTEND_URL=http://localhost:5173

# Environment
NODE_ENV=development